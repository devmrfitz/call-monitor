{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "baseUrl": "src",
    "declaration": false,
    "esModuleInterop": true,
    "inlineSourceMap": false,
    "lib": ["ESNext", "DOM"],
    "listEmittedFiles": false,
    "listFiles": false,
    "moduleResolution": "node",
    "noFallthroughCasesInSwitch": true,
    "pretty": true,
    "rootDir": "src",
    "skipLibCheck": true,
    "strict": true,
    "target": "esnext",
    "traceResolution": false,
    "outDir": "dist"
  },
  "compileOnSave": false,
  "exclude": ["node_modules", "dist"],
  "include": ["src"],
  "references": [
    {
      "path": "../auth"
    },
    {
      "path": "../secrets"
    },
    {
      "path": "../agents"
    },
    {
      "path": "../storage"
    },
    {
      "path": "../callmanager"
    },
    {
      "path": "../logger"
    },
    {
      "path": "../providers"
    },
    {
      "path": "../numbers"
    },
    {
      "path": "../domains"
    },
    {
      "path": "../projects"
    },
    {
      "path": "../users"
    },
    {
      "path": "../common"
    },
    {
      "path": "../core"
    }
  ]
}
